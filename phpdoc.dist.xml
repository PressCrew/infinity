<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
  <title>Infinity API Manual</title>
  <parser>
    <default-package-name>Infinity</default-package-name>
    <target>etc/phpdoc/output</target>
    <markers>
      <item>TODO</item>
      <item>FIXME</item>
    </markers>
    <extensions>
      <extension>php</extension>
      <extension>php3</extension>
      <extension>phtml</extension>
    </extensions>
    <visibility></visibility>
  </parser>
  <transformer>
    <target>etc/phpdoc/output</target>
  </transformer>
  <logging>
    <level>warn</level>
    <paths>
      <default>{APP_ROOT}/data/log/{DATE}.log</default>
      <errors>{APP_ROOT}/data/log/{DATE}.errors.log</errors>
    </paths>
  </logging>
  <files>
    <directory>.</directory>
    <ignore>activity/*</ignore>
    <ignore>assets/*</ignore>
    <ignore>blogs/*</ignore>
    <ignore>dashboard/assets/*</ignore>
    <ignore>dashboard/docs/*</ignore>
    <ignore>dashboard/templates/*</ignore>
    <ignore>engine/ICE/assets/*</ignore>
    <ignore>engine/ICE/lib/*</ignore>
    <ignore>engine/documents/*</ignore>
    <ignore>etc/*</ignore>
    <ignore>forums/*</ignore>
    <ignore>groups/*</ignore>
    <ignore>members/*</ignore>
    <ignore>registration/*</ignore>
  </files>
</phpdoc>
